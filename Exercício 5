def S(s):
  if s == "a" or s == "b":
      return True
  if s.startswith("a") and S(s[1:]):
      return True
  if s.endswith("b") and S(s[:-1]):
      return True
  return False


def pertence(c):
  if S(c):
    print(f'Cadeia = ({c}) pertence a S')
  else:
    print(f'Cadeia = ({c}) nÃ£o pertence a S')


conjunto = ["a", "ab", "aba", "aaab", "bbbbb"]
for c in conjunto:
  pertence(c)
